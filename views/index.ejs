<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title><%=title%></title>
</head>
<body>
  <h1><%=title%></h1>
  <%poetry.forEach(function(art){%>
   <div>
     <h1><%=art.title%></h1>
     <div><%=art.author%></div>
     <div><%=art.content%></div>
    </div>
  <% })%>
</body>
</html>